{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
        ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },

    "rules": {
    },
    
      "settings": {
        "react": {
          "version": "detect"
        }
      },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
}
